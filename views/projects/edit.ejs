<h1>Edit Route</h1>
<div class="ui main text container segment">
        <div class="ui huge header">New Project</div>
        <form class="ui form" action="/projects/<%= project.id %>?_method=PUT" method="POST">
            <div class="field">
                <label>Name</label>
                <input type="text" name="project[name]" value="<%= project.name%>">
            </div>
            <div class="field">
                <label>Image</label>
                <input type="text" name="project[image]" value="<%= project.image%>>
            </div>
            <div class="field">
                <label>Description</label>
                <textarea name="project[description]"><%= project.description %></textarea>
            </div>
            <input type="submit" class="ui violet basic button big">
        </form>
    </div>